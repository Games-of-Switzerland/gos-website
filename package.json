{
  "name": "gos-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "storybook:start": "start-storybook -s ./public -p 6006"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.1",
    "@testing-library/jest-dom": "^5.7.0",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.0.1",
    "classnames": "^2.2.6",
    "kitsu": "^9.0.6",
    "kitsu-core": "^9.0.6",
    "next": "^9.4.0",
    "postcss-cli": "^7.1.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-dom": "16.13.1",
    "react-popper-2": "npm:react-popper@^2.2.3",
    "react-use": "^14.2.0",
    "sass": "^1.26.5",
    "tailwindcss": "^1.7.3",
    "throttle-debounce": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.0.16",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-backgrounds": "^6.0.16",
    "@storybook/addon-docs": "^6.0.16",
    "@storybook/addon-knobs": "^6.0.16",
    "@storybook/addon-links": "^6.0.16",
    "@storybook/react": "^6.0.16",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.2",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.35",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-dom": "^16.9.8",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ],
  "prettier": {
    "trailingComma": "es5",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!**/node_modules/**"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "module-resolver",
            {
              "root": [
                "./"
              ],
              "alias": {
                "components": "./src/components"
              }
            }
          ]
        ]
      }
    }
  }
}
